[run]
source = imgc, main.py
omit = 
    */tests/*
    */__pycache__/*
    .venv/*
    build/*
    dist/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:

[html]
directory = coverage_html

[xml]
output = coverage.xml
